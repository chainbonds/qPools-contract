{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "esModuleInterop": true,
    "declaration": true,
    "outDir": "./lib",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strictPropertyInitialization": false,
    "forceConsistentCasingInFileNames": true,
    "rootDir": "src",
    "strict": true,
    "noEmit": true,
    "isolatedModules": true
  },
  "include": [
    "src"
  ],
  "lib": ["dom", "dom.iterable", "esnext", "es2017"],
  "exclude": ["node_modules", "../dapp-nextjs/*", "../qpools-admin-sdk/*", "../solbond/*"]
}
